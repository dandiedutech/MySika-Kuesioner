{
  "manifest_version": 3,
  "name": "MySika Auto Kuesioner",
  "version": "1.2",
  "description": "Mengisi kuesioner EDOM dan Layanan di SIKA Unindra secara otomatis lalu menyimpannya.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://sika.unindra.ac.id/kuesioner*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}